{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/**/*.style.{js,jsx}",
      "!src/node_modules/",
      "!src/config/*.js",
      "!src/routes/*.jsx",
      "!src/setupTests.js",
      "!src/jest.config.js",
      "!src/{index,history,config,theme,reportWebVitals}.js",
      "!src/store/index.js",
      "!src/reportWebVitals.js"
    ]
  },
  "dependencies": {
    "@emotion/react": "^11.10.0",
    "@emotion/styled": "^11.10.0",
    "@mui/icons-material": "^5.8.4",
    "@mui/material": "^5.10.0",
    "@reduxjs/toolkit": "^1.8.4",
    "axios": "^0.27.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.34.0",
    "react-redux": "^8.0.2",
    "react-router-dom": "^6.3.0",
    "react-scripts": "5.0.1",
    "redux": "^4.2.0",
    "redux-saga": "^1.1.3",
    "web-vitals": "^2.1.0"
  },
  "scripts": {
    "start": "REACT_APP_STAGE=dev react-scripts start",
    "build": "react-scripts build",
    "build:prod": "REACT_APP_STAGE=prod GENERATE_SOURCEMAP=false react-scripts build",
    "test": "react-scripts test",
    "unit": "CI=true react-scripts test --silent --env=jsdom --coverage --coverageDirectory=./output/coverage --colors --runInBand",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.3.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/user-event": "^14.4.3",
    "redux-devtools-extension": "^2.13.9"
  }
}
